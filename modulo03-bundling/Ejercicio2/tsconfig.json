{
    "compilerOptions": {
        "target": "ES6",
        "module": "ES6",
        "moduleResolution": "Node",
        "declaration": false,
        "noImplicitAny": false,
        "allowSyntheticDefaultImports": true,
        "sourceMap": true,
        "jsx": "react",
        "noLib": false,
        "skipLibCheck": true,
        "esModuleInterop": true
    },
    "include": [
        "./src/**/*"
    ],
    "exclude": [
        "./node_modules"
    ]
}